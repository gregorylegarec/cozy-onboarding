{
  "name": "Onboarding",
  "version": "1.0.0",
  "main": "src/main.jsx",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config.js --bail",
    "clean": "rm -rf build",
    "copy:manifest": "install -m0644 package.json README.md LICENSE build",
    "deploy": "git-directory-deploy --username Cozy --email contact@cozycloud.cc --directory build/ --branch build --repo=https://$GITHUB_TOKEN@github.com/cozy/cozy-onboarding-v3.git",
    "tx": "tx pull --all || true",
    "postbuild": "npm run copy:manifest",
    "prebuild": "npm-run-all lint clean tx",
    "pretest": "npm run lint",
    "prewatch": "npm run clean",
    "lint": "standard 'src/**/*.js' 'src/**/*.jsx'",
    "test": "mocha test/**/* --bail --reporter spec --compilers coffee:coffee-script/register --colors",
    "test:browserstack": "(cd test/specs/ && ../../node_modules/.bin/wdio wdio.conf.js)",
    "watch": "NODE_ENV=development webpack --config webpack.config.js --watch --display-modules --display-chunks",
    "watch:server": "NODE_ENV=development webpack-dev-server --config webpack.config.js --port 8082 --display-modules --display-chunks --inline --hot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/{github_username}/{github_slug}.git"
  },
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "contributors": [
    "gregorylegarec"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/{github_username}/{github_slug}/issues"
  },
  "homepage": "https://github.com/{github_username}/{github_slug}#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.0.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-env": "^1.0.2",
    "babel-runtime": "^6.0.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cozy-client-js": "^0.2.0",
    "css-loader": "^0.26.0",
    "css-mqpacker": "^4.0.1",
    "enzyme": "^2.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "git-directory-deploy": "^1.5.0",
    "html-webpack-plugin": "^2.24.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.0.0",
    "json-loader": "^0.5.0",
    "node-fetch": "^1.6.3",
    "npm-run-all": "^3.1.0",
    "postcss": "^5.2.0",
    "postcss-discard-duplicates": "^2.0.2",
    "postcss-loader": "^1.1.0",
    "sinon": "^1.17.0",
    "standard": "^8.6.0",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.0",
    "stylus-loader": "^2.4.0",
    "svg-sprite-loader": "^0.0.31",
    "wdio-browserstack-service": "^0.1.4",
    "wdio-mocha-framework": "^0.5.9",
    "webdriverio": "^4.6.2",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.0",
    "webpack-merge": "^1.0.2"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "backbone.marionette": "2.4.6",
    "baconjs": "^0.7.89",
    "classnames": "^2.2.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.12.3",
    "cozy-ui": "3.0.0-beta1",
    "fetch-mock": "^5.9.3",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jquery": "^3.1.1",
    "jsdom-global": "^2.1.1",
    "marionette": "^0.0.0",
    "mocha": "^3.2.0",
    "node-polyglot": "^2.2.0",
    "underscore": "^1.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "styl"
    ],
    "moduleNameMapper": {
      "\\.(png|gif|jpe?g|svg)$": "<rootDir>/test/__mocks__/fileMock.js",
      "styles": "identity-obj-proxy"
    }
  }
}
